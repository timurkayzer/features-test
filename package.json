  "bugs": {
    "url": "https://github.com/timurkayzer/features-test/issues"
  },
  "homepage": "https://github.com/timurkayzer/features-test#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "express": "^4.18.1",
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  }
}
